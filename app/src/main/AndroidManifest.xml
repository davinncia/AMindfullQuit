<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.davinciapp.amindfullquit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MainThemeNoActionBar">
        <activity android:name="com.davinciapp.amindfullquit.developer.DeveloperActivity" />
        <activity android:name="com.davinciapp.amindfullquit.principles.PrinciplesActivity" />
        <activity android:name="com.davinciapp.amindfullquit.settings.SettingsActivity" />
        <activity android:name="com.davinciapp.amindfullquit.smoking.SmokingActivity"
            android:theme="@style/SmokingThemeNoActionBar" />
        <activity android:name="com.davinciapp.amindfullquit.meditation.MeditationActivity"/>
        <activity android:name="com.davinciapp.amindfullquit.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


    </application>

</manifest>